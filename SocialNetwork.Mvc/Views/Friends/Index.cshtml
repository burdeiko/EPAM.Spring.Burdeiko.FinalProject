@model IEnumerable<SocialNetwork.Mvc.Models.PersonViewModel>

@{
    ViewBag.Title = "Friends";
}


<div class="container">
    <ul class="nav nav-tabs">
        <li>@Html.ActionLink("MyFriends", "Index", "Friends")</li>
        <li>@Html.ActionLink("Incoming Requests", "IncomingRequests", "Friends")</li>
        <li>@Html.ActionLink("Outgoing Requests", "OutcomingRequests", "Friends")</li>
    </ul>
    @foreach(var item in Model)
    {
        @Html.Partial("FriendPartial", item)
    }
</div>

